{
  "name": "typelens",
  "version": "0.0.0",
  "private": true,
  "description": "Debug TypeScript types like you debug code",
  "repository": {
    "type": "git",
    "url": "https://github.com/lyw405/typeLens.git"
  },
  "keywords": [
    "typescript",
    "type-checker",
    "developer-tools",
    "vscode-extension",
    "type-inspector",
    "type-diff"
  ],
  "author": "TypeLens Contributors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lyw405/typeLens/issues"
  },
  "homepage": "https://github.com/lyw405/typeLens#readme",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "typecheck": "turbo run typecheck"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "esbuild": "^0.19.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.0",
    "turbo": "^1.11.0",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.15.0"
